        text-indent: 0;
        top: 0;
    }

    .form-horizontal-radiobutton-input-row .a-radio-fancy .a-icon-radio {
        float: left;
        position: absolute;
        left: 50%;
        transform: translate(-50%, 0);
    }

    .form-options {
        margin: 0 auto;
        max-width: 450px;
    }

    .form-inner-container {
        z-index: 1;
    }

