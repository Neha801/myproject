<script type="text/javascript">
    P.when('jQuery', 'ready').execute('customer-feedback-card-form_53b600ca-742c-46db-aeff-112df48ef569', function($) {
        var formSelector = '#form_53b600ca-742c-46db-aeff-112df48ef569';
        var mainFieldSelector = 'input[name="fieldResponses[0].contents[0]"]';

        $(mainFieldSelector, formSelector).click(function() {
            clearErrorMessage(this);
        });

        $('.form-submit-button', formSelector).click(function(event) {
            event.preventDefault();
            $('input', this).blur();

            if(validateRadioButtons()) {
                $(formSelector).addClass('form-disabled');
                $.ajax({
                    url:  '/forms/submit',
                    type: 'post',
                    data:  $(formSelector).serialize()
                }).then(onSubmitSuccess, onSubmitFailure);
            }
        });

        function updateResponseMessage() {
            var $selectedRadioButton = $(mainFieldSelector + ':checked', formSelector);
            var selectedValue = $selectedRadioButton.closest(".a-radio").data('index');
            var responseClass = selectedValue > 3 ? 'high' : 'low';
            $('.response-message', formSelector).attr('data-result', responseClass);
        }

        function clearErrorMessage(field) {
            $(field).closest('.form-horizontal-radiobutton-container').removeAttr('data-error');
        }

        function validateRadioButtons() {
            var isValid = true;
            var radioButtonContainers = $('.form-horizontal-radiobutton-container.required', formSelector);
            radioButtonContainers.each(
                function(index) {
                    var numChecked = $('input:radio:checked', this).length;
                    if (numChecked === 0) {
                        $(this).attr('data-error', 'empty');
                        isValid = false;
                    }
                });
            return isValid;
        }

        function onSubmitSuccess(data) {
            if (data.successful) {
                updateResponseMessage();
                $(formSelector).addClass('submitted');
                $(formSelector).removeClass('form-disabled');
            } else {
                onSubmitFailure();
            }
        }

        function onSubmitFailure(data) {
            $(formSelector).attr('data-error', 'submission');
            $(formSelector).removeClass('form-disabled');
        }
    });
</script>

