        content: '';
        border-bottom: 2px solid #ccc;
        width: 100%;
        position: absolute;
        top: 50%;
        left: 50%;
        vertical-align: middle;
        transform: translate(0, -100%);
        z-index: -1;
    }

    .form-horizontal-radiobutton-input-row .form-table-column:last-child:after {
        display: none;
    }

    .form-horizontal-radiobutton-container .field-missing {
        text-align: center;
